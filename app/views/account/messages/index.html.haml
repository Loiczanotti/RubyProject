.row
  .col-xs-10.col-xs-offset-1
    %table.table.table-striped
      %thead
        %tr
          %th{:style => "width: 60px"} Image
          %th{:style => "width: 60px"} Price
          %th{:style => "width: 200px"} Date
          %th Message
      %tbody
        -# - raise @contacts.inspect
        - @contacts.each do |contact|
          %tr
            %td= link_to image_tag contact.listing.picture, width: 100
            %td= contact.listing.price
            %td= contact.created_at
            %td= contact.message
